                        fieldLabel="Awesomebar Type"
                        name="./awesomeBarType"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <responsive
                                jcr:primaryType="nt:unstructured"
                                text="Responsive"
                                value="responsive"/>
                            <normal
                                jcr:primaryType="nt:unstructured"
                                text="Normal"
                                value="normal"/>
                        </options>
                    </awesomebartype>
                    <hideFeedbackSection
                        jcr:mixinTypes="[rep:AccessControllable]"
                        jcr:primaryType="cq:Widget"
                        fieldDescription=" This will hide feedback section displaying just above footer."
                        fieldLabel="Show Feedback Section"
                        inputValue="true"
                        name="./hideFeedbackSection"
                        type="checkbox"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <feedback
                                jcr:primaryType="nt:unstructured"
                                text="Show Feedback Section"
                                value="yes"/>
                        </options>
                    </hideFeedbackSection>
                    <background
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Background color to be applied on Feedback Section. Default set to &lt;b>#d1d3d4&lt;/b>"
                        fieldLabel="Feedback Section Background Color"
                        name="./feedbackBGColor"
                        showHexValue="{Boolean}true"
                        xtype="colorfield"/>
