                        defaultValue="_self"
                        fieldDescription="To be used in Top Navigation and Navigation Bar"
                        fieldLabel="Launch Type"
                        name="./launchType"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <self
                                jcr:primaryType="nt:unstructured"
                                text="Same Window"
                                value="_self"/>
                            <new
                                jcr:primaryType="nt:unstructured"
                                text="New Window"
                                value="_blank"/>
                        </options>
                    </launchType>
                    <pagestyle
