                        name="./hideinnav"
                        type="checkbox"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <top
                                jcr:primaryType="nt:unstructured"
                                text="Top"
                                value="top"/>
                            <left
                                jcr:primaryType="nt:unstructured"
                                text="Left"
                                value="left"/>
                            <navbar
                                jcr:primaryType="nt:unstructured"
                                text="Navigation Bar"
                                value="navbar"/>
                        </options>
                    </hideinnav>
                    <hideTopNav
                        jcr:primaryType="cq:Widget"
                        defaultValue="parent"
                        fieldDescription=" "
                        fieldLabel="Hide Top Navigation"
                        name="./hideTopNav"
                        type="radio"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="No"
                                value="no"/>
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Yes"
                                value="yes"/>
                            <parent
                                jcr:primaryType="nt:unstructured"
                                text="As in Parent"
                                value="parent"/>
                        </options>
                    </hideTopNav>
                    <hideLeftNav
                        jcr:primaryType="cq:Widget"
                        defaultValue="parent"
                        fieldDescription=" "
                        fieldLabel="Hide Left Navigation"
                        name="./hideLeftNav"
                        type="radio"
                        xtype="hidden">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="No"
                                value="no"/>
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Yes"
                                value="yes"/>
                            <parent
                                jcr:primaryType="nt:unstructured"
                                text="As in Parent"
                                value="parent"/>
                        </options>
                    </hideLeftNav>
                    <hideHeader
                        jcr:primaryType="cq:Widget"
                        path="/apps/mcd/components/page/global/dialog/items/items/props/items/hideHeader.infinity.json"
                        xtype="cqinclude">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="No"
                                value="no"/>
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Yes"
                                value="yes"/>
                            <parent
                                jcr:primaryType="nt:unstructured"
                                text="As in Parent"
                                value="parent"/>
                        </options>
                    </hideHeader>
                    <resType
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./topnavimage/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                    <launchType
