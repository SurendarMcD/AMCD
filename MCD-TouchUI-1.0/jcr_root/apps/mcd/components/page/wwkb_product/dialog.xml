                                    <inside_x0020_MCD
                                        jcr:primaryType="nt:unstructured"
                                        text="Inside Mcdonalds"
                                        value="20051"/>
                                    <pcitesting
                                        jcr:primaryType="nt:unstructured"
                                        text="PCI Testing"
                                        value="20113"/>
                                </options>
                            </Category>
                            <views
                                jcr:primaryType="cq:Widget"
                                defaultValue="global"
                                fieldDescription="&lt;br>"
                                fieldLabel="View(s)"
                                name="./pci/entry1/View"
                                type="checkbox"
                                width="{Long}300"
                                xtype="pciviews">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <australia
                                        jcr:primaryType="nt:unstructured"
                                        text="Australia"
                                        value="AU"/>
                                    <global
                                        jcr:primaryType="nt:unstructured"
                                        text="Global"
                                        value="ENT"/>
                                    <unitedstates
                                        jcr:primaryType="nt:unstructured"
                                        text="United States"
                                        value="US"/>
                                    <newzealand
                                        jcr:primaryType="nt:unstructured"
                                        text="New Zealand"
                                        value="NZ"/>
                                </options>
                            </views>
                            <Title
                                jcr:primaryType="nt:unstructured"
                                fieldDescription="Defaults to Page Title"
                                fieldLabel="Title"
                                inputValue="{Boolean}true"
                                name="./pci/entry1/Title"
                                width="300"
                                xtype="textfield"/>
                            <Description
                                jcr:primaryType="nt:unstructured"
                                fieldDescription="Defaults to Page Description"
                                fieldLabel="Description"
                                name="./pci/entry1/Description"
                                rows="4"
                                width="300"
                                xtype="textarea"/>
