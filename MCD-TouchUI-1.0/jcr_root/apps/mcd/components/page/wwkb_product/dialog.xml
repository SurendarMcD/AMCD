                        optionsCallback="function(path,record) { var pageHandle=path.replace('/jcr:content','.common.html?type=productDaypart');&#xa;this.setOptions(CQ.Util.formatData(CQ.Util.eval(CQ.HTTP.get(pageHandle)))); }"
                        type="checkbox"
