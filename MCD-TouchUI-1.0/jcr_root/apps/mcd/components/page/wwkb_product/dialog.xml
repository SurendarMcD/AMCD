                    <menuItemRole
                        jcr:primaryType="cq:Widget"
                        defaultValue="limitedtimeoffer"
                        fieldDescription="(Ctrl+click to select multiple)"
                        fieldLabel="Menu Item Role"
                        name="./WWKBMenuItemRole"
                        optionsCallback="function(path,record) { var pageHandle=path.replace('/jcr:content','.common.html?type=menuItemRole');&#xa;this.setOptions(CQ.Util.formatData(CQ.Util.eval(CQ.HTTP.get(pageHandle)))); }"
                        type="checkbox"
                        xtype="selection"/>
