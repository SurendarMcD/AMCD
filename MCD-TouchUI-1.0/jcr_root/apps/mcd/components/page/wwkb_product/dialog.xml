                            jcr:primaryType="nt:unstructured"
                            selectionchanged="function(box,value){  var dialog=this.findParentByType('dialog');&#xa;var Data=CQ.Util.eval(CQ.HTTP.get(dialog.path.replace('/jcr:content','.common.html?type=areaOfTheWorld&amp;country=true&amp;aowValue='+value)));&#xa;dialog.getField('./WWKBCountry').setOptions(CQ.Util.formatData(Data));&#xa;dialog.getField('./WWKBCountry').setValue(Data[0].value);}"/>
